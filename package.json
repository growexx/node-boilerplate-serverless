{
  "name": "growexx-node-boiler-plate",
  "version": "0.1.0",
  "description": "Growexx node boiler plate for API",
  "main": "index.js",
  "scripts": {
    "prestart": "NODE_ENV=local node_modules/migrate-mongo/bin/migrate-mongo.js up",
    "up": "NODE_ENV=local migrate-mongo up",
    "start": "NODE_ENV=local nodemon .",
    "test": "NODE_ENV=testing nyc mocha test/alltests.js --exit",
    "jsdoc": "./node_modules/.bin/jsdoc server/* -r  --destination jsdocs/jsdocs",
    "commit": "git-cz"
  },
  "author": "Growexx",
  "license": "ISC",
  "dependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "aws-sdk": "^2.864.0",
    "body-parser": "^1.17.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "dotenv": "^8.2.0",
    "express": "^4.15.3",
    "helmet": "^3.21.3",
    "i18n": "^0.8.3",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.21",
    "method-override": "^2.3.9",
    "migrate-mongo": "^7.0.1",
    "moment": "^2.19.2",
    "mongoose": "^5.12.0",
    "mongoose-aggregate-paginate-v2": "^1.0.4",
    "mongoose-paginate-v2": "^1.3.16",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.5.0",
    "randomstring": "^1.1.5",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "jsdoc": "^3.6.6",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "sinon": "^10.0.0",
    "source-map-support": "^0.5.19",
    "supertest": "^6.1.3"
  },
  "nyc": {
    "lines": 5,
    "statements": 5,
    "functions": 5,
    "branches": 5,
    "check-coverage": true,
    "exclude": [
      "node_modules",
      "**/test/**",
      "coverage",
      "migrations",
      "jsdocs",
      ".eslintrc.js",
      "migrate-mongo-config.js",
      "util/country.js",
      "util/currency.js",
      "util/timeZone.js",
      "util/languageISO.js",
      "util/http-status.js"
    ],
    "reporter": [
      "lcov",
      "html"
    ],
    "cache": true,
    "all": true
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/Growexx-master/Growexx-api.git"
  },
  "release": {
    "repositoryUrl": "https://bitbucket.org/Growexx-master/Growexx-api.git",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "publish": [
      {
        "path": "@semantic-release/npm",
        "npmPublish": false,
        "tarballDir": "dist"
      }
    ]
  },
  "homepage": "https://bitbucket.org/ZsigDevelopment/semver-demo.git#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
